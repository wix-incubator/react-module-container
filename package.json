{
  "name": "react-module-container",
  "version": "1.0.1486",
  "description": "Small library, that enables building large-scale application with lazy-loaded modules based on either React or Angular.",
  "homepage": "https://github.com/wix/react-module-container#readme",
  "private": false,
  "author": {
    "name": "Shahar Talmi",
    "email": "shahart@wix.com",
    "url": ""
  },
  "scripts": {
    "start": "yoshi start --entry-point=./dist/test/mock/fake-server.js",
    "build": ":",
    "precommit": "yoshi lint",
    "pretest": "yoshi build",
    "test": "yoshi test",
    "posttest": "yoshi lint",
    "release": "yoshi release"
  },
  "files": [
    "dist"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "yoshi": {
    "entry": {
      "react-module-container": "./react-module-container.ts",
      "demo": "./demo/demo.tsx",
      "module": "./demo/module.tsx",
      "lazy/angular-module": "./lazy/angular-module.ts",
      "lazy/react-module": "./lazy/react-module.tsx",
      "demo-4": "./demo/demo-4.scss",
      "demo-5": "./demo/demo-5.scss",
      "demo-shared": "./demo/demo-shared.scss",
      "umd": "./index.ts"
    },
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM"
    },
    "exports": "reactModuleContainer"
  },
  "devDependencies": {
    "@types/angular": "^1.7.2",
    "@types/lodash": "^4.14.158",
    "@types/node": "^10.17.28",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^15.6.28",
    "@types/react-dom": "^15.5.12",
    "@types/react-router": "^2.0.57",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^4.2.0",
    "express": "^4.13.4",
    "jsdom": "16.2.2",
    "jsdom-global": "3.0.2",
    "react-redux": "^5.1.2",
    "react-router": "^2.8.1",
    "redux": "^4.0.5",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "typescript": "^3.9.7",
    "yoshi": "^1.2.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      [
        "babel-plugin-transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@types/react-redux": "^5.0.21",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2"
  }
}
